{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 3,
  "settings": {
    "appearanceTools": true,
    "border": {
      "color": false,
      "radius": false,
      "style": false,
      "width": false
    },
    "color": {
      "background": true,
      "custom": true,
      "customDuotone": true,
      "customGradient": true,
      "defaultDuotone": false,
      "defaultGradients": false,
      "defaultPalette": false,
      "duotone": [],
      "gradients": [],
      "link": true,
      "palette": [
        {
          "color": "#2b2b2b",
          "name": "Base",
          "slug": "base"
        },
        {
          "color": "#1a318a",
          "name": "Contrast",
          "slug": "contrast"
        },
        {
          "color": "#1f1b1a",
          "name": "Primary",
          "slug": "primary"
        },
        {
          "color": "#ffffff",
          "name": "Background",
          "slug": "background"
        }
      ],
      "text": true
    },
    "custom": {},
    "dimensions": {},
    "layout": {},
    "position": { "sticky": false },
    "shadow": { "defaultPresets": false },
    "spacing": {
      "spacingScale": {
        "steps": 1
      },
      "blockGap": null
    },
    "typography": {
      "fluid": true,
      "customFontSize": false,
      "dropCap": true,
      "fontStyle": true,
      "fontWeight": true,
      "letterSpacing": false,
      "lineHeight": false,
      "fontFamilies": [
        {
          "name": "Main",
          "slug": "main",
          "fontFamily": "GeorgiaPro-CondRegular, sans-serif",
          "fontFace": [
            {
              "fontFamily": "GeorgiaPro-CondRegular",
              "fontWeight": "400 500 700",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "src": ["file:./assets/fonts/GeorgiaPro-CondRegular.ttf"]
            }
          ]
        },
        {
          "name": "Primary",
          "slug": "primary",
          "fontFamily": "GeorgiaPro-CondLight, sans-serif",
          "fontFace": [
            {
              "fontFamily": "GeorgiaPro-CondLight",
              "fontWeight": "300 400 500 700",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "src": ["file:./assets/fonts/GeorgiaPro-CondLight.ttf"]
            }
          ]
        },
        {
          "name": "Quotable",
          "slug": "quotable",
          "fontFamily": "Jaipur, sans-serif",
          "fontFace": [
            {
              "fontFamily": "Jaipur",
              "fontWeight": "400 500 700",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "src": ["file:./assets/fonts/Jaipur.ttf"]
            }
          ]
        }
      ],
      "fontSizes": [
        {
          "name": "Small",
          "slug": "small",
          "size": "16px"
        },
        {
          "name": "Medium",
          "slug": "medium",
          "size": "20px"
        },
        {
          "name": "Large",
          "slug": "large",
          "size": "40px"
        },
        {
          "name": "Extra Large",
          "slug": "x-large",
          "size": "60px"
        }
      ]
    },
    "useRootPaddingAwareAlignments": false,
    "blocks": {
      "core/button": {
        "border": {
          "radius": true
        }
      },
      "core/pullquote": {
        "border": {
          "color": true,
          "radius": true,
          "style": true,
          "width": true
        }
      },
      "core/image": {
        "lightbox": {
          "enabled": true,
          "allowEditing": true
        }
      }
    }
  },
  "styles": {
    "color": {
      "text": "#2b2b2b",
      "background": "#ffffff"
    },
    "elements": {
      "button": {
        "color": {
          "text": "#ffffff",
          "background": "#1a318a"
        },
        ":hover": {
          "color": {
            "background": "#364b9c"
          }
        }
      },
      "link": {
        "color": {
          "text": "var:preset|color|contrast"
        }
      }
    },
    "blocks": {
      "core/group": {
        "color": {
          "text": "var:preset|color|base",
          "background": "var:preset|color|background"
        }
      },
      "core/image": {
        "border": {
          "radius": "8px"
        }
      },
      "core/heading": {
        "shadow": "0 1px 2px 0 rgb(0 0 0 / 0.05)"
      },
      "core/paragraph": {
        "typography": {
          "fontFamily": "GeorgiaPro-CondRegular, sans-serif",
          "fontSize": "1.25rem",
          "fontStyle": "normal",
          "fontWeight": "500",
          "letterSpacing": "0",
          "lineHeight": "1.6",
          "textDecoration": "none"
        }
      }
    }
  },
  "customTemplates": [],
  "templateParts": [
    {
      "area": "header",
      "name": "header",
      "title": "Header"
    },
    {
      "area": "loop",
      "name": "loop",
      "title": "Loop"
    },
    {
      "area": "footer",
      "name": "footer",
      "title": "Footer"
    }
  ],
  "patterns": []
}
